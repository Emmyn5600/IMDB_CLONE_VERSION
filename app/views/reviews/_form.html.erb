<%= simple_form_for([@movie, @movie.reviews.build]) do |f|%>
<%= f.label :rate%>
<%= f.number_field :rating, max: 10, min:1, class: "form-control" %>
<%= f.input :comment%>
<%= f.button :submit, class: "btn btn-outline-success mt-2" %>
<%= link_to 'Back to Movie', movie_path(@movie), class: "btn btn-outline-primary mt-2"%>
<%end%>
